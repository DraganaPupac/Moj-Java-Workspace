
public class App {

	public static void main(String[] args) {
		// 

	int value = 0;
	
	while(value < 10)
	{
		System.out.println("Broj je: " + value);
		value = value + 1;
	}
    
	 while(value < 10)
    {
        System.out.println("Iteracija broj: " + value);
        
        value = value - 1;
    }
		
	}

}
